{
  "name": "git-recover-staged",
  "version": "1.0.0",
  "description": "A git extension to recover your staged file after a hard reset: $ git reset --hard && git recover-staged",
  "scripts": {
    "recover": "ts-node src/index.ts --esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SnakeLil/git-recover-staged.git"
  },
  "keywords": [
    "git",
    "recover",
    "reset",
    "recover-staged"
  ],
  "author": "lilsnake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SnakeLil/git-recover-staged/issues"
  },
  "homepage": "https://github.com/SnakeLil/git-recover-staged#readme",
  "dependencies": {
    "commander": "^13.0.0",
    "inquirer": "^12.3.2",
    "ts-node": "^10.9.2"
  }
}
